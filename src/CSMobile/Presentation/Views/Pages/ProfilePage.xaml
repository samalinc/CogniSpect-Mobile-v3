<?xml version="1.0" encoding="utf-8"?>

<local:ProfilePageBase xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CSMobile.Presentation.Views.Pages;assembly=CSMobile.Presentation.Views"
             x:Class="CSMobile.Presentation.Views.Pages.ProfilePage"
             NavigationPage.HasNavigationBar="False">
    <Grid Margin="30">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>

        <Button Grid.Row="0" Grid.Column="0" Text="Tests" Command="{Binding OpenTestsCommand}"/>
    </Grid>
</local:ProfilePageBase>