<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewExtensions="clr-namespace:CSMobile.Presentation.Views.ViewExtensions;assembly=CSMobile.Presentation.Views"
             x:Class="CSMobile.Presentation.Views.Pages.Startup.StartupPage">
    <ContentPage.Content>
        <Image HorizontalOptions="FillAndExpand"
               VerticalOptions="FillAndExpand"
               Margin="10, 0, 10, 0"
               Aspect="AspectFit"
               Source="{viewExtensions:ImageResource CSMobile.Presentation.Views.Resources.Images.icon.png}" />
    </ContentPage.Content>
</ContentPage>